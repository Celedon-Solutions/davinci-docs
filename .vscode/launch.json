{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Mintlify Dev",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "mintlify",
                "dev"
            ],
            "console": "integratedTerminal"
        }
    ]
} 